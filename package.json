{
    "name": "assess",
    "displayName": "Assess",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "start": "cordova run ios --browserify --target=\"iPad-Retina, 10.2\" ",
        "build": "npm run reset_platform && cordova build --browserify",
        "test": "echo \"Error: no test specified\" && exit 1",
        "refresh_plugins": "npm install && cordova plugin remove assess-cordova-plugins && cordova plugin add cordova-plugins/ -save && cordova prepare --browserify",
        "reset_platform": "npm install && npm run refresh_plugins && cordova platform remove browser ios && cordova platform add browser ios --browserify && cordova prepare --browserify"
    },
    "author": "Q-interactive Team",
    "license": "Apache-2.0",
    "dependencies": {
        "assess-cordova-plugins": "file:cordova-plugins",
        "cordova-browser": "^5.0.3",
        "cordova-ios": "^4.5.4",
        "cordova-plugin-device": "^2.0.1",
        "cordova-plugin-file": "^6.0.1",
        "cordova-plugin-file-transfer": "^1.7.1",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "generator-fountain-webapp": "^1.0.0",
        "generator-webpack-2-typescript": "^3.2.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-file": {},
            "cordova-plugin-file-transfer": {},
            "cordova-plugin-device": {},
            "assess-cordova-plugins": {}
        },
        "platforms": [
            "browser",
            "ios"
        ]
    },
    "devDependencies": {}
}