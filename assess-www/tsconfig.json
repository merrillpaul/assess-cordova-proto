{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "importHelpers": true,
    "strict": false,
    "sourceMap": true,
    "lib": [
      "es5",
      "dom.iterable",
      "es2015.iterable",
      "scripthost",
      "webworker"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl":  "./",
    "paths": {
      "@assess/*": ["src/scripts/*"],
      "typedi": ["node_modules/typedi-no-dynamic-require"]
    }
  }
}


